{
  "version": 2,
  "buildCommand": "cd frontend && npx next build",
  "devCommand": "cd frontend && npx next dev",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}

